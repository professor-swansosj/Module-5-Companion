<?xml version="1.0" encoding="UTF-8"?>
<!-- Sample NETCONF Configuration XML -->
<!-- This demonstrates XML structure used in NETCONF operations -->

<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
    <interface>
      <name>GigabitEthernet1/0/1</name>
      <description>Sample Interface Configuration</description>
      <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
      <enabled>true</enabled>
      <ipv4 xmlns="urn:ietf:params:xml:ns:yang:ietf-ip">
        <enabled>true</enabled>
        <address>
          <ip>192.168.10.1</ip>
          <netmask>255.255.255.0</netmask>
        </address>
      </ipv4>
    </interface>
    
    <interface>
      <name>Loopback0</name>
      <description>Management Loopback</description>
      <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:softwareLoopback</type>
      <enabled>true</enabled>
      <ipv4 xmlns="urn:ietf:params:xml:ns:yang:ietf-ip">
        <enabled>true</enabled>
        <address>
          <ip>10.1.1.1</ip>
          <netmask>255.255.255.255</netmask>
        </address>
      </ipv4>
    </interface>
  </interfaces>
  
  <system xmlns="urn:ietf:params:xml:ns:yang:ietf-system">
    <hostname>CATALYST-SW01</hostname>
    <contact>Network Operations Team</contact>
    <location>Building A - Server Room</location>
    
    <clock>
      <timezone-name>America/New_York</timezone-name>
    </clock>
    
    <ntp>
      <enabled>true</enabled>
      <server>
        <name>pool.ntp.org</name>
        <transport>
          <udp>
            <address>pool.ntp.org</address>
            <port>123</port>
          </udp>
        </transport>
        <association-type>server</association-type>
      </server>
    </ntp>
    
    <dns-resolver>
      <server>
        <name>primary-dns</name>
        <udp-and-tcp>
          <address>8.8.8.8</address>
        </udp-and-tcp>
      </server>
      <server>
        <name>secondary-dns</name>
        <udp-and-tcp>
          <address>8.8.4.4</address>
        </udp-and-tcp>
      </server>
      <search>company.local</search>
      <search>company.com</search>
    </dns-resolver>
  </system>
</config>